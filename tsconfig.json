{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "declaration": true,
    "outDir": "./dist",
    "jsx": "react",
    "esModuleInterop": true,
    "lib": ["es2015"],
    "inlineSourceMap": true
  },
  "typedocOptions": {
    "exclude": "src/__tests__/**/*",
    "out": "docs",
    "mdPagesSourcePath": "pageDocs",
    "theme": "markdown-pages"
  }
}
